<h1>Contact</h1>

<hr>

<%= form_for(:contact, url: notify_path) do |f| %>
  <%= f.label :email %>
  <%= f.text_field  :email %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :subject %>
  <%= f.text_field :subject %>
  <%= f.label :message %>
  <%= f.text_area :message %>

  <%= f.submit "Submit" %>
<% end %>
